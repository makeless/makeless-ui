{
  "root": true,
  "env": {
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "@vue/typescript"
  ],
  "plugins": [
    "eslint-plugin-tsdoc"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "rules": {
    "max-len": "off",
    "tsdoc/syntax": "warn"
  }
}
